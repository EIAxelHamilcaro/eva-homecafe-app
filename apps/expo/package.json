{
  "name": "expo",
  "version": "1.0.0",
  "main": "./App.tsx",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "dev:clean": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "type-check": "tsc --noEmit",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod": "eas build --profile production --platform all",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "update": "eas update --branch preview",
    "submit": "eas submit --platform all"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.67.3",
    "clsx": "^2.1.1",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "lucide-react-native": "^0.562.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-hook-form": "^7.69.0",
    "react-native": "0.81.5",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "^15.15.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@packages/typescript-config": "workspace:*",
    "@types/react": "~19.1.17",
    "@types/react-dom": "^19.1.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  }
}
